{
  "name": "@koroner616/materialize-k",
  "description": "Fork privado de Materialize CSS adaptado para uso interno.",
  "author": "Koroner616",
  "version": "2.2.2",
  "style": "dist/css/materialize.css",
  "sass": "sass/materialize.scss",
  "typings": "dist/js/materialize.d.ts",
  "license": "MIT",
  "main": "dist/js/materialize.js",
  "module": "./dist/js/materialize.mjs",
  "exports": {
    ".": {
      "import": "./dist/js/materialize.mjs",
      "require": "./dist/js/materialize.cjs.js"
    }
  },
  "files": [
    "dist/css",
    "dist/js",
    "sass",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "pnpm build && jasmine-browser-runner runSpecs",
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "dev": "vite",
    "release": "pnpm lint && pnpm build -- --environment BUILD:release && node ci/compress.mjs",
    "preversion": "pnpm lint && pnpm test",
    "version": "pnpm build -- --environment BUILD:release && node ci/compress.mjs && git add ."
  },
  "lint-staged": {
    "js/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "Chrome >= 30",
    "Firefox >= 30",
    "Safari >= 8"
  ],
  "engine": "node >= 18",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.5.2",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.10.0",
    "jasmine": "^5.1.0",
    "jasmine-browser-runner": "^2.5.0",
    "jasmine-core": "^5.2.0",
    "postcss": "^8.4.49",
    "rollup": "^4.20.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "^1.77.8",
    "tslib": "^2.6.3",
    "typescript": "^5.4.3",
    "typescript-eslint": "^8.6.0",
    "vite": "^7.1.4"
  }
}
